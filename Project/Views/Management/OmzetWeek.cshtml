@{
    ViewBag.Title = "OmzetWeek";
}

<div class="prod_box_big">
    <div class="top_prod_box_big"></div>
    <div class="center_prod_box_big">
        <div class="product_title_big">Omzet per week</div>

        <table class="beheertab" style="width: 100%">
            <thead>
                <tr>
                    <th>Weeknr:</th>
                    <th>Omzet</th>
                </tr>
            </thead>
            <tbody>
                @for (int i = 0; i < Model.ListManager.Count; i++)
                {
                    <tr>
                        <td>@Model.ListManager[i].week</td>
                        <td>&euro; @String.Format("{0:0.00}",Model.ListManager[i].totaal)</td>
                    </tr>
                }
            </tbody>
        </table>

        <p><a href="/Management/Management" style="text-align: right; height: 27px; background-color: white; width: 195px; line-height: 27px; color: #1c4a52; border-radius: 10px; padding: 7px; text-decoration: none;">Vorige</a></p>
    </div>
    <div class="bottom_prod_box_big"></div>
</div>